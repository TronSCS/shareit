pragma solidity ^0.4.2;

contract freebie 
{struct User{
    uint id;
    string name;
    uint token;}

mapping (address => bool)public user;

event eventaddUser(
uint indexed Userid);
constructor()}

function addUser(string_name) private{
    require(!user{msg.sender});
    require(uint id > 0;)
    user[msg.sender] = true;
    
    user[_Userid].token +++++;}